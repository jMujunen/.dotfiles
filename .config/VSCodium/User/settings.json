{
  //Colors
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#AF7A30",
    "tab.unfocusedActiveBorder": "#00ccff25",
    "tab.border": "#00ccff25",
    "tab.inactiveBackground": "#222b2e75",
    "tab.activeBackground": "#af7a302f",
    "tab.activeBorderTop": "#AF7A30",
    "tab.inactiveForeground": "#646464",
    "titleBar.border": "#00ccff25",
    "panelTitle.activeBorder": "#00ccff25",
    "activityBar.border": "#00ccff25",
    "sideBar.border": "#00ccff25",
    "editorHint.border": "#00ccffe7",
    "commandCenter.activeBorder": "#79115fb9",
    "terminal.border": "#00ccff25",
    "terminal.tab.activeBorder": "#00ccff25",
    "sideBarSectionHeader.border": "#00ccff25",
    "panel.border": "#00ccff25",
    "panelSection.border": "#00ccff25",
    "panel.dropBorder": "#00ccff25",
    "editor.selectionBackground": "#ff00d42a",
    "editor.selectionHighlightBorder": "#066d9ca4",
    "editor.inactiveSelectionBackground": "#ff00004f",
    "editor.wordHighlightBorder": "#700101d8",
    "editor.findMatchBorder": "#066fac",
    "editorSuggestWidget.border": "#0299be65",
    "editorSuggestWidget.selectedBackground": "#0859798e",
    "input.border": "#00ccff25",
    "inputOption.activeBorder": "#00ccff25",
    "menu.selectionBorder": "#00ccff25",
    "menu.border": "#ff000038",
    "panelInput.border": "#00ccff25",
    "panelTitle.activeForeground": "#00ccff25",
    "panelSectionHeader.border": "#00ccff25",
    "selection.background": "#755379",
    "statusBar.border": "#00ccff25",
    "sideBySideEditor.horizontalBorder": "#00ccff25",
    "sideBySideEditor.verticalBorder": "#00ccff25",
    "widget.border": "#00ccff25",
    "focusBorder": "#ff000080",
    "dropdown.border": "#00ccff20"
  },
  // Theme
  "workbench.iconTheme": "vscode-icons",
  "workbench.colorTheme": "Everforest Dark",
  "everforest.darkContrast": "hard",

  // <- Look and Feel ->
  //  ----------------
  // Font
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.fontSize": 14,

  // Function
  "vsicons.dontShowNewVersionMessage": true,
  "task.allowAutomaticTasks": "on",
  "workbench.cloudChanges.autoResume": "off",
  "editor.inlineSuggest.enabled": true,
  "editor.insertSpaces": true,
  "editor.indentSize": "tabSize",
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingComments": "beforeWhitespace",
  "editor.autoClosingDelete": "always",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.autoIndent": "advanced",
  "window.zoomLevel": 0,
  "editor.trimAutoWhitespace": true,
  "search.useParentIgnoreFiles": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "block-outline",
  "editor.cursorWidth": 0,
  "editor.smoothScrolling": true,
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.cursorSurroundingLines": 0,
  "editor.suggest.showCustomcolors": true,
  "editor.suggest.snippetsPreventQuickSuggestions": true,
  "editor.formatOnPaste": true,
  "editor.accessibilitySupport": "off",
  "editor.minimap.enabled": false,
  "editor.codeLensFontSize": 4,
  "editor.experimentalInlineEdit.enabled": true,
  "editor.suggest.filterGraceful": true,
  "editor.quickSuggestionsDelay": 1,
  "editor.suggest.matchOnWordStartOnly": false,
  // Hide menu bar, set activity bar location and disable minimap
  "window.menuBarVisibility": "toggle",
  "workbench.activityBar.location": "hidden",
  // Adjust word wrapping and indentation
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingIndent": "deepIndent",
  "editor.wordWrapColumn": 120,
  "editor.rulers": [90, 100],
  "editor.tabSize": 4,
  "editor.quickSuggestions": {
    "other": true,
    "comments": false,
    "strings": false
  },

  // Explorer
  "explorer.confirmDragAndDrop": false,
  "explorer.sortOrder": "type",
  //exclude
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.DS_Store": true,
    "**/.vscode": true,
    "**/__pycache__": true,
    "**/*env": true
  },

  // Workbench
  "workbench.startupEditor": "none",
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.editor.dragToOpenWindow": true,
  "workbench.tree.indent": 16,
  "workbench.editor.enablePreview": true,
  "workbench.enableExperiments": true,
  "workbench.editor.preventPinnedEditorClose": "keyboard",
  "workbench.editor.splitOnDragAndDrop": true,
  "workbench.hover.delay": 200,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.pinnedTabsOnSeparateRow": false,
  "workbench.editor.pinnedTabSizing": "compact",
  "workbench.editor.closeOnFileDelete": true,

  // <- Git ->
  // --------
  "git.enableSmartCommit": true,
  "git.confirmSync": false,

  // <- Formatting ->
  // ------------------
  // General
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  // Per file type
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2,
    "editor.formatOnSave": true
  },
  "[ini]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[qml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[sh]": {
    "editor.defaultFormatter": "foxundermoon.shell-format",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": false
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },

  // <- Extentions ->
  // ---------------
  "extensions.ignoreRecommendations": false,
  "extensions.autoUpdate": "onlySelectedExtensions",

  // Enable better comments and set tags
  "better-comments.multilineComments": true,
  "better-comments.highlightPlainText": false,
  "better-comments.tags": [
    { "tag": "#", "color": "#18b566" },
    { "tag": "^", "color": "#00e6dc" },
    { "tag": "!", "color": "#FF2D00" },
    { "tag": "?", "color": "#3498DB" },
    { "tag": "//", "strikethrough": true },
    { "tag": ["todo", "to-do"], "color": "#FF8C00" },
    { "tag": "-", "color": "#75D6B3" },
    { "tag": "*", "color": "#0d7e33" },
    { "tag": ["DEBUG", "debug", "DEBUGGING", "debugging"], "color": "#f3ff1d" },
    { "tag": ["WARNING", "warning", "WARN", "warn"], "color": "#f3ff1d" },
    { "tag": ["ERROR", "error"], "color": "#ff0000" },
    { "tag": ["INFO", "info"], "color": "#00b8ff" },
    { "tag": ["HINT", "hint"], "color": "#00b8ff" },
    { "tag": ["FIXME", "fixme"], "color": "#ff0000" },
    { "tag": ["NOTE", "note"], "color": "#00b8ff" }
  ],

  //Continue
  "continue.enableTabAutocomplete": true,
  "continue.showInlineTip": true,
  "continue.telemetryEnabled": false,

  //Todo tree

  "todo-tree.highlights.defaultHighlight": {
    "todo-tree.filtering.excludeGlobs": [
      "**/vendor/**",
      "**/node_modules/**",
      "**/dist/**",
      "**/bower_components/**",
      "**/build/**",
      "**/.vscode/**",
      "**/.github/**",
      "**/_output/**",
      "**/*.min.*",
      "**/*.map"
    ],
    "todo-tree.general.tags": [
      "BUG",
      "FIXME",
      "INCOMPLETE",
      "TODO",
      "NOTE",
      "[ ]",
      "[x]"
    ],
    "icon": "alert",
    "type": "text",
    "foreground": "#FF8C00",
    "background": "#272e33",
    "opacity": 50,
    "iconColour": "#21749bb7",
    "gutterIcon": true,
    "todo-tree.regex.enableMultiLine": true,
    "todo-tree.general.statusBar": "tags",
    "todo-tree.highlights.enabled": true
  },
  "todo-tree.highlights.customHighlight": {
    "TODO": {
      "icon": "checklist",
      "type": "line",
      "iconColour": "#FF8C00"
    },
    "FIXME": {
      "foreground": "#ce1010",
      "iconColour": "#ce1010",
      "gutterIcon": true,
      "background": "#272e33",
      "fontWeight": "bold",
      "icon": "x-circle"
    },
    "NOTE": {
      "icon": "note",
      "foreground": "#1d7bb9",
      "background": "#272e33",
      "iconColour": "#1d7bb9",
      "gutterIcon": true
    },
    "INCOMPLETE": {
      "icon": "alert",
      "foreground": "#9d54ef",
      "background": "#272e33",
      "iconColour": "#9d54ef",
      "gutterIcon": true
    },
    "BUG": {
      "icon": "bug",
      "foreground": "#e5e909",
      "background": "#272e33",
      "iconColour": "#e5e909",
      "gutterIcon": true
    },
    "[ ]": {
      "icon": "check",
      "foreground": "#96ca15",
      "background": "#272e33",
      "iconColour": "#96ca15",
      "gutterIcon": true
    },
    "[x]": {
      "icon": "check",
      "foreground": "#10a63d",
      "background": "#272e33",
      "iconColour": "#10a63d",
      "gutterIcon": true
    },
    "- [ ]": {
      "icon": "check",
      "foreground": "#96ca15",
      "background": "#272e33",
      "iconColour": "#96ca15",
      "gutterIcon": true
    },
    "- [x]": {
      "icon": "check",
      "foreground": "#10a63d",
      "background": "#272e33",
      "iconColour": "#10a63d",
      "gutterIcon": true
    }
  },

  // <- Other ->
  // -----------
  // Terminal settings
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.profiles.linux": {
    "zsh": {
      "path": "/usr/bin/zsh",
      "args": [],
      "icon": "terminal",
      "title": "zsh",
      "isDefault": true,
      "color": "terminal.ansiGreen"
    }
  },

  // Configure markdown-pdf output directory and conversion type
  "markdown-pdf.outputDirectoryRelativePathFile": true,
  "markdownConverter.ConversionType": ["HTML", "PDF"],
  "shellcheck.ignorePatterns": {
    "**/*.bash*": true,
    "**/*.shellrc": true
  }
}

